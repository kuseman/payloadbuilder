    <version>0.0.1-SNAPSHOT</version>
    <packaging>pom</packaging>
    <name>Payloadbuilder</name>
    <description>Query engine</description>

    <properties>
        <!-- Build -->
        <maven.compiler.source>11</maven.compiler.source>
